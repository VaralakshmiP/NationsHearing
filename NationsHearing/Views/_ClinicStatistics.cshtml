<div class="clinic_statistics">
    <div class="clinic_statistics_header">
        <label class="clinic_statistics_header_label">Clinic Stats</label>
        <select class="clinic_statistics_header_select">
            <option>This Week</option>
            <option>Last Week</option>
        </select>
    </div>
    <div class="clinic_statistics_body">
        @for ( var i =1; i<=3; i++)
        {
        <div class="clinic_statistics_body_individual">
            <label class="clinic_statistics_body_individual_number">20</label>
            <label class="clinic_statistics_body_individual_type">Testing Appointments</label>
        </div>
        }
    </div>
</div>