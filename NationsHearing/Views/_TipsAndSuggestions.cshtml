@{
    List<Dictionary<string, string>> tipsList = new List<Dictionary<string, string>>();
    tipsList.Add(new Dictionary<string, string>{
        { "suggestion"," Wednesday seems to be your busiest day" },
        {"action", "Extend Work Hours" }
    });
    tipsList.Add(new Dictionary<string, string>{
        { "suggestion"," NH members seem to prefer morning appointments" },
        {"action", "Some call to action" }
    });

    tipsList.Add(new Dictionary<string, string>{
        { "suggestion"," NH members seem to prefer morning appointments" },
        {"action", "Some call to action" }
    });
}
<div class="tips_suggestions_section">
    <img class="tips_suggestions_img" src="~/images/background.png" />
    <div class="tips_suggestions_header">
        Tips And Suggestions
    </div>
    <div class="tips_suggestions_desc">
        Some recommendations to improve your business
    </div>
    <div class="tips_suggestions_list">
        @foreach(var tip in tipsList)
        {
        <div class="tips_suggestions_list_individual">
            <div class="tips_suggestions_list_individual_desc">
                @tip["suggestion"]
            </div>
            <div class="tips_suggestions_list_individual_action">
                <button class="tips_suggestions_list_individual_action_button">@tip["action"]</button>
            </div>
        </div>
        }

    </div>
</div>
